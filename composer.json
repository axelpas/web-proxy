{
	"name" : "dootech/web-proxy",
	"type" : "application",
	"description" : "An advanced web proxy built with Symfony components, Goutte and Guzzle",
	"keywords" : [
		"proxy"
	],
	"homepage" : "https://dootech.com",
	"license" : "MIT",
	"authors" : [{
			"name" : "Ha Ho",
			"email" : "hoducha@gmail.com"
		}
	],
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/hoducha/Goutte.git"
      }
    ],
	"require" : {
		"fabpot/goutte" : "dev-master",
		"symfony/http-foundation" : ">=4",
		"symfony/http-kernel" : ">=4",
		"symfony/event-dispatcher" : ">=4",
        "guzzlehttp/guzzle": "6.1.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "4.7.5",
		"symfony/debug" : ">=4",
        "psy/psysh": "@stable"
	},
	"autoload" : {
		"psr-4" : {
			"Dootech\\WebProxy\\" : "src/",
            "Dootech\\WebProxy\\Test\\" : "tests/"
		}
	}
}
